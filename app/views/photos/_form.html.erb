<% form_for @photo, html: { multpart: true } do |f| %>

	<div class="form-group">
		<%= f.label :title %>
		<%= f.text_field :title, class: "form-control" %>
	</div>

	<div class="form-group">
		<%= f.label :image %>
		<%= f.file_field :image, class: "form-control" %>
	</div>

	<div class="actions">
		<%= f.submit class:"btn btn-primary" %>
	</div>

<% end %>